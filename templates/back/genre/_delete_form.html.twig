<form method="post"
      action="{{ path('app_admin_genre_delete', {'id': genre.id}) }}"
      onsubmit="return confirm('Êtes vous sûr de vouloir supprimer ce genre ?');"
>
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ genre.id) }}">
    <button class="btn">Delete</button>
</form>
