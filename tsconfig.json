{
  "exclude": [
    "node_modules",
    "vendor"
  ],
  "compilerOptions": {
    "sourceMap": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "es2019",
      "dom",
      "dom.iterable",
      "es2015.proxy"
    ]
  }
}
